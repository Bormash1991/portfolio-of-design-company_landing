{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": ["eslint:recommended", "plugin:jquery/slim"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["babel", "jquery"],
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "caughtErrorsIgnorePattern": "^ignore"
      }
    ],
    "no-case-declarations": "warn",
    "linebreak-style": "off",
    "no-var": "warn",
    "babel/new-cap": 1,
    "babel/camelcase": 1,
    "babel/no-invalid-this": 1,
    "babel/object-curly-spacing": 0,
    "babel/semi": 1,
    "babel/no-unused-expressions": 1,
    "babel/valid-typeof": 1
  }
}